{
    "execute": [
        {
            "action": "load_folder",
            "dir": "base"
        },
        {
            "action": "board", 
            "file": ".indexer/board.md"
        },
        {
            "action": "links",
            "dir": ".indexer/links"
        },
        {
            "action": "thumbs", 
            "width": 320,
            "height": 180
        },
        {
            "action": "index", 
            "intro": "intro.md",
            "file": "categorias.md",
            "group_by": "categories"
        },
        {
            "action": "summary", 
            "file": "summary.md",
            "group_by": "categories"
        },
        {
            "action": "view", 
            "file": "view.md",
            "group_by": "categories",
            "intro": "intro.md",
            "sort_by": "fulltitle",
            "reverse_sort": false,
            "posts_per_row": 4,
            "empty_fig": null
        },
        {
            "action": "posts",
            "dir": "_posts",
            "base_raw_remote": "https://raw.githubusercontent.com/senapk/senapk.github.io/master/base",
            "default_date": "1984-04-25",
            "categories_dir": "category"
        },
        {
            "action": "load_folder",
            "dir": "drafts"
        },
        {
            "action": "board", 
            "file": ".indexer/board_drafts.md"
        },
        {
            "action": "view",
            "file": "drafts.md",
            "group_by": "categories",
            "posts_per_row": 4
        }

    ]
}
